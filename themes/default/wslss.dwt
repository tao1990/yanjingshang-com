	<script>
	// 滚动
		$('#section-container').fullpage({
			scrollingSpeed:500,
			verticalCentered:false,
			anchors: ['page1', 'page2', 'page3', 'page4', 'page5', 'page6', 'page7'],
			menu:'#menu',
			onLeave:function(index, direction){
				$('#headerNav').addClass('fixNav')
				direction=='down' ? $('#headerNav').hide() : $('#headerNav').show()
			}
		})
	// 自适应高度
	$(window).load(function(){
		// 形式一
		var sections=$('.section-container')
		sections.each(function(){
			var height=$(this).height();
			$(this).css('marginTop',-height/2)
		})
		try{
			// 形式二
			var sections2=$('.section-fluid')
			var screenH=$(window).height()
			sections2.each(function(){
				var selfHeight=$(this).height()
				var fixHeight=$(this).siblings('.section-fixed').height()
				$(this).css('paddingTop',(screenH-fixHeight-selfHeight)/2+'px')
			})
		}catch(e){}

		// 底部控制
		if(document.documentElement.clientHeight<=700){
			$('.footer-wrapper').addClass('smallScreenFix')
		}
			


	})
	</script>
