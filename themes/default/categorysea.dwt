<script type="text/javascript">
$(document).ready(function(){
	//$("img").lazyload({placeholder:"/js/img/white.gif", effect:"fadeIn"});
	
	getData(searchAttr); //初始化载入数据
	
	//browser_resize();
	$(window).resize(function(){
		//browser_resize();
	});
	
	//显示下拉导航
	$(".cate_title").mouseenter(function(){
		$(".dropdown").css("display", "block");
	});
	$(".cate_title").mouseleave(function(){
		$(".dropdown").css("display", "none");
	});
	
	//打开下拉导航子项
	$(".submenu").mouseenter(function(){
		var li_index = $(".submenu").index(this); //当前li的索引值
		$(this).addClass('submenu_over submenu_over' + li_index);
		$(this).find('.extensive_menu').removeClass('disn').addClass('ext_menu' + li_index); //展开扩展菜单
		$(this).find('.hidden_left_line').removeClass('disn'); //遮住子菜单与扩展菜单相连的边框
	});
	$(".submenu").mouseleave(function(){
		var li_index = $(".submenu").index(this);
		$(this).removeClass('submenu_over submenu_over' + li_index);
		$(this).find('.extensive_menu').addClass('disn');
		$(this).find('.hidden_left_line').addClass('disn');
	});
	
	//取消选择的属性
	$(".selected_item dd div").click(function(){
		var item_index = $(".selected_item dd div").index(this);
		$(this).addClass("disn");
		
		searchAttr[item_index] = "";
		getData(searchAttr);
		
		//更改样式
		$("#item_" + item_index).find(".item_all").addClass("item_selected"); //选中全部
		$("#item_" + item_index).find("span").removeClass("item_selected"); //移除子项选中
	});
	
	//选择属性
	$(".items dd span").click(function(){
		var attr_value = $(this).text(); //属性值
		var item_name = $(this).parents(".items").find(".item_name").text(); //属性组名称
		var temp = $(this).parents(".items").attr("id").split("_"); //属性组DIV的id
		var item_index = temp[1];
		
		//<div id="attr_0" class="disn">品牌：<span>博士伦蕾丝<b></b></span></div>
		$("#attr_" + item_index).html(item_name + "<span>" + attr_value + "<b></b></span>");
		$("#attr_" + item_index).removeClass("disn");
		
		searchAttr[item_index] = attr_value;
		getData(searchAttr);
		
		//更改样式
		$(this).parents(".items").find(".item_all").removeClass("item_selected"); //取消全部
		$(this).parents("dl").find("span").removeClass("item_selected"); //移除本组所有选中项
		$(this).addClass("item_selected"); //选中本项
	});
	
	//选择“全部”
	$(".item_all").click(function(){
		$(this).addClass("item_selected");
		var temp = $(this).parents(".items").attr("id").split("_"); //属性组DIV的id
		var item_index = temp[1];
		
		$("#attr_" + item_index).addClass("disn");
		searchAttr[item_index] = '';
		getData(searchAttr);
		
		$(this).parents(".items").find("span").removeClass("item_selected");
	});
	
	//清除所有选中项reset_seleted
	$(".reset_seleted").click(function(){
		$(".item_all").addClass("item_selected");
		$(".selected_item dd div").addClass("disn");
		$(".item_list dd span").removeClass("item_selected");
		
		searchAttr[0] = ""; //大类ID
		searchAttr[1] = "";
		searchAttr[2] = "";
		searchAttr[3] = "";
		searchAttr[4] = "";
		searchAttr[5] = "";
		searchAttr[6] = "";
		searchAttr[7] = "";
		searchAttr[8] = "";
		searchAttr[9] = "";
		searchAttr[10] = "";
		searchAttr[11] = "";
		searchAttr[12] = "";
		searchAttr[13] = "";
		
		getData(searchAttr);
	});
	
	//展开,收起选项
	$("#slide_btn").click(function(){
		$(".toggle_panel").slideToggle("slow");
		if ($(this).attr("class") == "slide_hidden") {
			$(this).removeClass("slide_hidden");
			$(this).html("收起 &and;");
		} else {
			$(this).addClass("slide_hidden");
			$(this).html("显示全部选项 &or;");
		}
	});
	
});

//ajax获取商品数据
function getData(searchAttr) {
	var params = searchAttr.join("||"); //将数组用||号连接
	var keyword = $("#search_keyword").val();
	$.ajax({
		type: "POST",
		url: "ajax_goods.php?act=category_goods&parent_id=0" + "&page="+page+"&order_by="+order_by+"&params=" + encodeURIComponent(params)+"&search=1&keyword=" + keyword,
		success:function(d){
			$("#ajax_result").html(d);
		},
		error:function(d){
			alert('error:'+d.toSource());
		}
	});
}

//resize page width
function browser_resize() {
	var browser_width = $(document).width();
	if (browser_width < 1350) {
		$("#top_main").removeClass("w1210");
		$("#top_main").addClass("w990");
	} else {
		$("#top_main").removeClass("w990");
		$("#top_main").addClass("w1210");
	}
}

</script>



