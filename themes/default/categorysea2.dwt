<script type="text/javascript" language="javascript">
function chooseOption(option, value, id, param, fromId, group) {
	if (lastId == "") {
		//第一次点击选择的选项
		lastId = fromId;
		groupid = group;
	} else {
		$("#"+lastId).css("display", "inline-block");
		lastId = fromId;
	}
	
	$("#"+id).css("display", "inline-block");
	$("#"+id).html("<a href='#focus_selected' onclick='document.getElementById(\""+fromId+"\").style.display=\"inline-block\";this.parentNode.style.display=\"none\";'>" + option + "：<span class='col0ca'>" + value + "<b></b></span></a>");
	
	if (id == 'selected_brandx') {
		selectedAttr[0] = param;
		selectedAttr[7] = "id=" + fromId.replace("selected_brandx_", "");
	}
	if (id == 'selected_attr0') selectedAttr[1] = param;
	if (id == 'selected_attr1') selectedAttr[2] = param;
	if (id == 'selected_attr2') selectedAttr[3] = param;
	if (id == 'selected_attr3') selectedAttr[4] = param;
	if (id == 'selected_attr4') selectedAttr[5] = param;
	if (id == 'selected_price') selectedAttr[6] = param;
	
	//将js数组用&号连接
	var url = selectedAttr.join("&");
	//alert(url);
	//alert(encodeURI(url));
	
	//刷新数据
	$(document).ready(function() {
		$.ajax({
			type: 'POST',
			url: 'goods_list_ajax.php?' + encodeURI(url+'&keyword={$keyword}'),
			success:function(d){
				$("#ajax_result").html(d);		
			},
			error:function(d){
				alert('error:'+d.toSource());
			} 
		});
	});
	
}

//取消选择
function cancelChoose(id) {
	if (id == 'selected_brandx') {
		selectedAttr[0] = "brandx=0";
		selectedAttr[7] = "id={$cat_ids}"; //如果取消品牌选项，则返回栏目ID
	}
	if (id == 'selected_attr0') selectedAttr[1] = "attr0=0";
	if (id == 'selected_attr1') selectedAttr[2] = "attr1=0";
	if (id == 'selected_attr2') selectedAttr[3] = "attr2=0";
	if (id == 'selected_attr3') selectedAttr[4] = "attr3=0";
	if (id == 'selected_attr4') selectedAttr[5] = "attr4=0";
	if (id == 'selected_price') selectedAttr[6] = "price_min=0&price_max=0";

	 var url = selectedAttr.join("&");
	
	//刷新数据
	$(document).ready(function() {
		$.ajax({
			type: 'POST',
			url: 'goods_list_ajax.php?' + encodeURI(url+'&keyword={$keyword}'),
			success:function(d){
				$("#ajax_result").html("" + d + "");				
			},
			error:function(d){
				alert('error:'+d.toSource());
			} 
		});
	});
}
</script>
</html>