    function convert(){

        var aa = $("#aa").find("option:selected").text();
        var bb = $("#bb").find("option:selected").text();
        var cc = $("#cc").find("option:selected").text();

        Ajax.call('/wechat/myopia_convert.php?act=ajax' , 'aa='+aa+'&bb='+bb+'&cc='+cc,convertResponse,'post','json');
    }

    function convertResponse(result){
        if(result.status == 2){
            var num  = result.textHtml;
            var rs = num.toFixed(2);
            $('#span').html(rs);
        }else{
            $('#span').html(result.textHtml);
        }
    }



