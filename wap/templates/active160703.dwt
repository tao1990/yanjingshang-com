<script>
// 楼层
function scroller(n,arr){
	$('html,body').animate({
		'scrollTop':arr[n]+'px'
	},200)
}
var sArr=[];
$(window).load(function(){
	$('.floor').each(function(){
		var self=$(this);
		sArr.push(parseInt(self.offset().top));
	})
})

function get_bonus(bonus_id){
    $.ajax({
        type:'POST',
        url:'ajax_active.php?act=get_bonus_160703',
        data:'&bonus_id='+bonus_id+'&m='+Math.random(),
        cache:false,
        success:
                function(da)
                {
                    if(da == 1){
                        alert('您已成功领取优惠券！请至我的红包/优惠券查看^_^');
                    }else if(da == 2){
                        alert('请登录后再试^_^');
                    }else if(da == 3){
                        alert('您已领取过该优惠券^_^');
                    }
                }
    });
}
