<script src="http://file.easeeyes.com/wap/js/wappop.js"></script>
<script src="http://file.easeeyes.com/index2015/js/lazyload.js"></script>
<script>
$("img").lazyload({placeholder:"http://img.easeeyes.com/index2015/white.gif", effect:"fadeIn"});
$(window).load(function(){
    // ---点击滚动 
    var ScrollArr=[],
        $scrolls=$(".J-tab-navs"),
        $scroll_nav=$(".J-tab-navs div");
    $scrolls.each(function(){
        ScrollArr.push(parseInt($(this).offset().top));
        $(this).find("div").each(function(i){
            $(this).attr("data-index",i);
        })
    });
    $scroll_nav.click(function(){
        $this=$(this);
        $("html,body").animate({"scrollTop":ScrollArr[$this.attr("data-index")]},500);
    })


    //---弹窗
    var pop=new Pop({id:"pop-01"});
        pop_main=document.getElementById('J-pop-box');
    document.getElementById("yhq-pop-btn").addEventListener("click",function(){
        pop.open();
        return false;
    },false);
    document.getElementById("pop-01").addEventListener('click',function(e){
        //alert()
        if(e.target!=pop_main){
            pop.close();
        }
    })

});    

</script>

