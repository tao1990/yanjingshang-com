var c_j = '{$format_ctime.j}';
var c_G = '{$format_ctime.G}';
var c_i = '{$format_ctime.i}';
window.setInterval(function(){ShowCountDown(c_Y, c_n, c_j, c_G, c_i, 'p_time', '');}, 1000);

</script>
