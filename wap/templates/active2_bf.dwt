<script type="text/javascript">
$(document).ready(function(){
	$("img").lazyload({placeholder:"/js/img/white.gif", effect:"fadeIn"});
	//-----每周活动 START----
	$('#weekly_active li').click(function() {
		window.open($(this).find('a').attr('href'));
	});
	$('#weekly_active li').mouseover(function() {
		$(this).siblings().removeClass('current_week').animate({width:"120px"}, "slow");
		$(this).siblings().animate({backgroundPosition:"0px"}, "fast");
		$(this).animate({width:"510px"}, "slow");
		$(this).animate({backgroundPosition:"-120px"}, "normal");
	});
	//---- 每周活动 END----
    
     $('#agree_view').bind('click',function(){
            $("#agree_div").fadeIn();
       
    })
	
	function get_bonus(bonus_id){
    
		$.ajax({
				type:'POST',
				url:'active.php?id=150801&act=get_bonus',
				data:'&bonus_id='+bonus_id+'&m='+Math.random(),							
				cache:false,					
				success:
				function(da)
				{
					 if(da == 1){
						 alert('您已成功领取优惠券！请至我的红包/优惠券查看^_^');
					 }else if(da == 2){
						 alert('请登录后再试^_^');
					 }else if(da == 3){
						 alert('您已领取过该优惠券^_^');
					 }
				}
			});
	}
	
	//--- 导航隐显控制 ---
	$('.active-nav').hide();
	$(window).scroll(function(){
		var scrollTop=document.documentElement.scrollTop=="0"?document.body.scrollTop:document.documentElement.scrollTop;
		if(parseInt(scrollTop)>=300){
			$('.active-nav').fadeIn(200);
		}else{
			$('.active-nav').fadeOut(200);
		}
	});
	
	//鼠标经过效果
	$(".colorbox img").hover(function(){
		$(this).stop().animate({
			"opacity":".6"
		},200)
	},function(){
		$(this).stop().animate({
			"opacity":"1"
		},200)
	});
	
	
});
</script>
