<script>
$(window).load(function(){
	var $nav=$('.navItems')
	var offTop=$nav.offset().top
	var sTop
	$(window).scroll(function(){
		sTop=document.documentElement.scrollTop || document.body.scrollTop
		if(sTop>=offTop){
			$nav.addClass('fixed')
		}else{
			$nav.removeClass('fixed')
		}
	})
});

function get_bonus(bonus_id){
