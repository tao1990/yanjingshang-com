<!-- #BeginLibraryItem "/library/header.lbi" --><!-- #EndLibraryItem -->
<div class="main">
    <div id="brand_2"><ul>
    <!--{foreach from=$ch_arr item=lc name=foo}-->
    	<li id="line{$lc.num}" style="width:100%; height:auto; border-bottom:1px dotted #e5e5e5;{if $lc.bnum eq 0}display:none;{/if}">
        	<dl>
            	<dd style="width:56px; height:56px; line-height:56px; border-right:1px dotted #e5e5e5; text-align:center; font-family:Arial; color:#cbcbcb; font-size:26px;">{$lc.char}</dd>
                <!--{foreach from=$brand item=lib name=foo}-->
                {if $lib.first_letter eq $lc.num}
                <dd><a href="{$lib.site_url}" title="{$lib.brand_name}" target="_blank"><img src="http://www.easeeyes.com/{$lib.brand_logo}" width="97" height="56" alt="{$lib.brand_name}"/></a></dd>
                {/if}
                <!--{/foreach}--> 
            </dl>
        </li>
    <!--{/foreach}--></ul>         
    </div><ul style="width:100%; height:10px;"></ul>    
</div>
<!-- #BeginLibraryItem "/library/footer.lbi" --><!-- #EndLibraryItem -->
