<script type="text/javascript">
function topan(num, bnum)
{
	if(bnum>0)
	{
		if($("#brand_2").css('display') == 'none')
		{
			$(".b_head > li").last().html('').addClass('bh2').prev().addClass('bh11').html('<a href="javascript:;" class="bh11"></a>');
			$("#brand_1").slideUp('slow');
			$("#brand_2").slideDown();	
			var tt = $("#brand_1").height();	
		}		
		var ph   = $("#letter_pan").height();
		var ltop = $("#line"+num).offset().top;	
		if(ltop>tt)
		{
			ltop = ltop - tt;
		}	
		ltop     = ltop-ph;
		$(document).scrollTop(ltop);
		var l_sn = num-65;
		$(".letter").eq(l_sn).addClass('letter2').siblings().removeClass('letter2');
	}
}

$(document).ready(function(){	
$("#letter_pan").smartFloat();//浮动
	

$(".b_head > li").first().click(function(){
	$(this).removeClass('bh11').html('<a href="javascript:;" class="bh1"></a>').next().removeClass('bh2').html('<a href="javascript:;" class="bh22"></a>');
	$("#brand_2").slideUp('slow');
	$("#brand_1").slideDown();	
});
$(".b_head > li").last().click(function(){
	$(this).html('').addClass('bh2').prev().addClass('bh11').html('<a href="javascript:;" class="bh11"></a>');
	$("#brand_1").slideUp('slow');
	$("#brand_2").slideDown();	
});

$('.bh11').hover(
	function(){
		//$(this).addClass('bh222');
		$(this).css("background-image","url(themes/default/images/brands/bt111.jpg)");
	},
	function(){
		$(this).css("background-image","url(themes/default/images/brands/bt11.jpg)");
	}
);
	
})

</script>
