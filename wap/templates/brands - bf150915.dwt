<style type="text/css">
	.main{width:100%; height:auto; margin:0 auto;}
	#brand_2{width:100%; height:auto; overflow:hidden;}
	.ad_8{ margin-bottom:0;}
	.b_head{/*width:990px;*/ height:34px; margin:15px auto;}
	.b_head li{width:102px; height:34px; margin-right:12px; cursor:pointer;}
	.bh1{background:url(../images/brands/bt1.jpg) no-repeat; width:102px; height:34px; margin-right:12px; display:block; }
	.bh11{background:url(../images/brands/bt11.jpg) no-repeat;}
	.bh11:hover{background:url(../images/brands/bt111.jpg) no-repeat;}

	.bh2{background:url(../images/brands/bt2.jpg) no-repeat;}	
	.bh22{background:url(../images/brands/bt22.jpg) no-repeat; width:102px; height:34px; margin-right:12px; display:block;}
	.bh22:hover{background:url(../images/brands/bt222.jpg) no-repeat;}
	
	.floatdiv{width:100%; height:66px;}
	#letter_pan{width:100%; height:64px; border:1px solid #d9d9d9; border-left:none; border-right:none; background-color:white; overflow:hidden;}
	.letter_dl{width:100%; height:26px; line-height:26px; margin:18px auto; overflow:hidden;}
	.letter_h{color:#b7b7b7; font-size:16px; letter-spacing:1px; text-indent:8px; width:106px;}
	
	.letter{width:26px; height:26px; line-height:26px; vertical-align:middle; background-color:#e8e8e8; color:#7d7d7d; font-size:24px; text-align:center; font-family:Arial; margin-right:8px;}
	.letter a{color:#7d7d7d; width:26px; height:26px; display:block;}
	.letter a:hover{color:white; background-color:#60d6e2; text-decoration:none; vertical-align:middle; text-align:center; width:26px; height:26px; display:block;}
	.letter2{color:white; background-color:#60d6e2; display:block; width:26px; height:26px;}	
	.letter2 a{color:white;}
	a{blr:expression(this.onFocus=this.blur())}
	
	.bpan{width:990px; height:auto; margin:30px auto 0 auto; background:url(../images/brands/bg.jpg) repeat-x #ededed; overflow:hidden;}
	.bpan_h1, .bpan_h2,.bpan_h3,.bpan_h31,.bpan_h4,.bpan_h5{width:990px; height:41px; background:url(../images/brands/brand_cat1.jpg) no-repeat;}
	.bpan_h2{background:url(../images/brands/brand_cat2.jpg) no-repeat;}
	.bpan_h3{background:url(../images/brands/brand_cat3.jpg) no-repeat;}
	.bpan_h31{background:url(../images/brands/brand_cat31.jpg) no-repeat;}
	.bpan_h4{background:url(../images/brands/brand_cat4.jpg) no-repeat;}
	.bpan_h5{background:url(../images/brands/brand_cat5.jpg) no-repeat;}
	.bp_ad{width:982px; height:180px; margin-left:4px;}
	.bp_ad li{width:491px; height:180px;}
	.brands_pan{width:980px; height:auto; min-height:57px; margin:6px 0 6px 5px; overflow:hidden;}
	.brands_pan li{width:97px; height:56px; border-bottom:1px solid #ededed; border-right:1px solid #ededed;}
    dl dd {float:left;}
    ul li {float:left; margin: 0;}
    #brand_2 ul li dl dd img{height: 57px;}
    .logo {width:36px; height: 36px; margin: 0; padding: 0}
</style>
