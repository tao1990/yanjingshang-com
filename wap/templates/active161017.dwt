<script>
var tabNav=new Tab({
  navs:document.querySelectorAll(".tabs-navs span"),
  tabs:document.querySelectorAll(".tabs-main")
})

$(window).load(function(){
    var navs=$('#tabs-navs')
    var navs_ot=navs.offset().top
    // 导航固定
    $(window).scroll(function(){
        var sTop=document.documentElement.scrollTop || document.body.scrollTop
        if(sTop>=navs_ot){
            navs.addClass('fixed')
        }else{
            navs.removeClass('fixed')
        }
    })
})



