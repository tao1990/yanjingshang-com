<script type="text/javascript">
$(document).ready(function(){
	$(".order_id_a").hover( 
		function(e){
			$("#goods_list").css({
						position:'absolute',
						left:e.pageX+10,
						top:e.pageY,
						cursor:'pointer',
						background:'white',
						color:'#333',
						display:'block'
					});
			//把数据加载进来
							
			var order_id = this.id;
			$.ajax({
				url:"user.php",
				data:"act=ajax_order_list&order_id="+order_id,
				cache:false,
				success:function(data){
					//alert(data);
					eval("var d ="+data);					
							
					//现在的是如何不数组数据传递到前台.
					var str = '';
					//把数据追加到表格当中
					
					//正常产品
					for(var i=0; i<d.length; i++){
						if( d[i]['extension_code'] != 'package_buy'){													
							var attr = '';
							if(d[i]['zselect'].length>0){ attr+='左眼:'+d[i]['zselect'];}						
							if(d[i]['yselect'].length>0){ attr+='右眼:'+d[i]['yselect'];}
							str += '<tr><td>'+d[i]['goods_name']+'</td><td>'+d[i]['goods_number']+'</td><td>'+d[i]['goods_price']+'</td><td>'+d[i]['goods_attr']+attr+'</td><td>'+d[i]['subtotal']+'</td></tr>';
							attr = '';
						}else{
							str += '<tr><td>'+d[i]['goods_name']+'</td><td>'+d[i]['goods_number']+'</td><td>'+d[i]['goods_price']+'</td><td>'+d[i]['goods_attr']+'</td><td>'+d[i]['subtotal']+'</td></tr>';
						}
					}
										
					$("#goods_list").append(str);
					str='';
		
				}
			});			
			
		},
		function(){
			//离开后		
	
			//把上个订单加载的东西去掉.鼠标放到商品列表中框仍然存在.
			$("#goods_list tr:not('.goods_list_head')").remove();			
			$("#goods_list").hide();
			
			/*
			$("#goods_list").mouseover(function(){
					
			});
			$("#goods_list").mouseout(function(){
					$("#goods_list tr:not('.goods_list_head')").remove();			
					$("#goods_list").hide();
			});*/
		} 
	);
});
</script>
