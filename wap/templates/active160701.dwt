.goodsItem-price .btn img{vertical-align:middle;}
.goodsItem-price del{color:#666;font-size:.8rem;}
.goodsItem-price .red{color:#ff3084;}
.goodsItem-price .red b{font-size:1.4rem;font-weight:100;}
.goodsItem-layer2{position:absolute;top:0;right:0;background:#f8404c;padding:.25rem;width:2.5rem;font-size:1rem;color:#fff;text-align:center;line-height:1;}
