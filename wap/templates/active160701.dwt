.goodsItem-title{margin-top:.5rem;line-height:2rem;height:2rem;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;color:#666;border-bottom:1px dashed #81817f;}
.goodsItem-price{padding-top:.5rem;overflow:hidden;color:#666;line-height:1rem;white-space:nowrap;font-size:.9rem;margin-top:.25rem;}
