<script type="text/javascript">
$(document).ready(function(){
	$(".product_more img, .product_tlf_tedian img, .mjx_pan img").lazyload({placeholder:"/js/img/white.gif", effect:"fadeIn"});
	
	//选中框架配镜
	//2014.08.13临时更改
	//$('.goods_append').click(function(){
	$('.goods_append').not(".not_click").click(function(){		

		var idx = $(this).parent().index();
		if(idx>0 && idx <7)
		{
			$("#get_glasses_type").val(idx);
		}
			
		//siblings hide
		$('.goods_append').each(function(i){
			var append_child = $(this).children();			
			if(append_child.hasClass("kj_ck_tip"))
			{				
				append_child.removeClass('kj_ck_tip');
				$(this).removeClass('goods_append_b');
			}
		});	
		$(this).addClass('goods_append_b').children().addClass('kj_ck_tip');	
		
		//change price
		var oprice = $('#old_goods_price').val();
		var tprice = 0;
		switch(idx)
		{
			case 2:
				tprice = 50;break;
			case 3:
				tprice = 100;break;	
			case 4:
				tprice = 320;break;	
			case 5:
				tprice = 560;break;	
			case 6:
				tprice = 780;break;	
			default:
				tprice = 0;  break;																				
		}
		nprice = (oprice*1+tprice);
		$('#goods_price').text(nprice+'.00');
	});	
});


