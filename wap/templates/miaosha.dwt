/**
 * 倒计时
 * 参数：截至时间的年、月、日、时、分, 以及操作的div名称、提示语
 */
function ShowCountDown(year, month, day, param_hour, param_minute, divname, tips) {
	var now = new Date();
	var endDate = new Date(year, month-1, day, param_hour, param_minute);
	var leftTime=endDate.getTime()-now.getTime();
	var leftsecond = parseInt(leftTime/1000);
	var day1=Math.floor(leftsecond/(60*60*24));
	var hour=Math.floor((leftsecond-day1*24*60*60)/3600);
	var minute=Math.floor((leftsecond-day1*24*60*60-hour*3600)/60);
	var second=Math.floor(leftsecond-day1*24*60*60-hour*3600-minute*60);
	var cc = document.getElementById(divname);

	//alert(day1 + '-' + hour + ':' + minute + ':' + second);
	if (day1 <= -1) {
		cc.innerHTML = '距离秒杀'+tips+'还有：<span class="time_number">0</span>天<span class="time_number">0</span>时<span class="time_number">0</span>分<span class="time_number">0</span>秒';
	} else {
		cc.innerHTML = '距离秒杀'+tips+'还有：<span class="time_number">'+day1+'</span>天<span class="time_number">'+hour+'</span>时<span class="time_number">'+minute+'</span>分<span class="time_number">'+second+'</span>秒';
	}
}
var c_Y = '{$format_ctime.Y}';
var c_n = '{$format_ctime.n}';
var c_j = '{$format_ctime.j}';
var c_G = '{$format_ctime.G}';
var c_i = '{$format_ctime.i}';
window.setInterval(function(){ShowCountDown(c_Y, c_n, c_j, c_G, c_i, 'current_end_time', '{$format_ctime.time_type}');}, 1000);

</script>
