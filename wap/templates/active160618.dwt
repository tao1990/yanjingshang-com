<script type="text/javascript">
function get_bonus(bonus_id){
    $.ajax({
        type:'POST',
        url:'ajax_active.php?id=160618&act=get_bonus_160618',
        data:'&bonus_id='+bonus_id+'&m='+Math.random(),
        cache:false,
        success:
                function(da)
                {
                    if(da == 1){
                        alert('您已成功领取优惠券！请至我的红包/优惠券查看^_^');
                    }else if(da == 2){
                        alert('请登录后再试^_^');
                    }else if(da == 3){
                        alert('您已领取过该优惠券^_^');
                    }else if(da == 4){
                        alert('您不是新会员，不能领取过该优惠券^_^');
                    }else if(da == 5){
                        alert('您不是老会员，不能领取过该优惠券^_^');
                    }
                }
    });
}

