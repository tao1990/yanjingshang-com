})
*/
// 导航悬浮
$(window).load(function(){
	var $fixNav=$('.navBar')
	var navTop=parseInt($fixNav.offset().top)
	$(window).scroll(function(){
		var sTop=document.documentElement.scrollTop || document.body.scrollTop
		if(parseInt(sTop)>=navTop){
			$fixNav.addClass('fixed')
		}else{
			$fixNav.removeClass("fixed")
		}
	})
})
function get_bonus(bonus_id){
    $.ajax({
        type:'POST',
        url:'ajax_active.php?act=get_bonus_161101_2',
        data:'&bonus_id='+bonus_id+'&m='+Math.random(),
        cache:false,
        success:
                function(da)
                {
                    if(da == 1){
                        alert('您已成功领取优惠券！请至我的红包/优惠券查看^_^');
                    }else if(da == 2){
                        alert('请登录后再试^_^');
                    }else if(da == 3){
                        alert('您已领取过该优惠券^_^');
                    }
                }
    });
}
// 导航滚动识别
$(window).load(function(){
  // 高度集合
  var heights=[],navLen
  var navHeight=parseInt($('.navBar').get(0).offsetHeight)
  //console.log(navHeight)
  $('.mainTab').each(function(){
      var $this=$(this)
      heights.push(parseInt($this.offset().top)-navHeight)
  })
  heights.reverse()
  navLen=heights.length
  // 判断
  function checkScrollTop(s){
      for(var i=0,len=heights.length;i<len;i++){
          if(s>=heights[i]){
              return navLen-1-i;
              break;
          }
      }
      return -1;
  }
  var sTop,
      $navItem=$('.navBar a')
  //console.log(heights)
  $(window).scroll(function(){
      sTop=document.documentElement.scrollTop || document.body.scrollTop
      var index=checkScrollTop(parseInt(sTop))
      if(index!=-1){
          $($navItem[index]).addClass('current').siblings().removeClass('current')
      }else{
          $navItem.removeClass('current')
      }
  })
})

</script>
